</head>
<body>

<main role="main" class="container">
    <div class="starter-template">
        <a href='/Home/Index'>Home</a> |

        <%if (reqInfo.authenticated) { %>
        <a href='/User/Profile'>Profile</a> | 

        <% } else { %>
          <a href='/User/Register'>Register</a> | 
        <% } %>

        <%if (reqInfo.authenticated) { %>
